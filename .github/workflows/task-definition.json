{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "nodejs",
    "containerDefinitions": [
        {
            "name": "mini-messageboard",
            "image": "public.ecr.aws/x4s6i0h4/mini-messageboard",
            "portMappings": [
                {
                    "name": "mini-meassageboard-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "awslogs-nodejs",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "awslogs-nodejs"
                }
            },
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "2 GB",
    "cpu": "1 vCPU",
    "executionRoleArn": "arn:aws:iam::732737896721:role/development"

}
