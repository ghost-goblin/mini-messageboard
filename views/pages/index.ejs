<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../pages/head'); %>
</head>
<body class="container">

<header>
  <%- include('../pages/header'); %>
</header>

<main>
  <div class="jumbotron">
    <ul>
      <% messages.forEach(function(message) { %>
        <li>
          <strong><%= message.text %></strong>
          <i><%= message.user %></i><br>
          <u><%= message.added %></u>
        </li>
      <% }); %>
    </ul>
  </div>
</main>

<footer>
  <%- include('../pages/footer'); %>
</footer>

</body>
</html>